[Unit]
Description=Framewave Proxy Container
Requires=updategraph.service 
After=updategraph.service swss.service syncd.service
StartLimitIntervalSec=1200
StartLimitBurst=3

[Service]
User={{ sonicadmin_user }}
ExecStartPre=/usr/local/bin/{{docker_container_name}}.sh start
ExecStart=/usr/local/bin/{{docker_container_name}}.sh wait
ExecStop=/usr/local/bin/{{docker_container_name}}.sh stop
ExecStopPost=/usr/local/bin/package_logs {{docker_container_name}}{% if multi_instance == 'true' %} %i{% endif %}

Restart=always
RestartSec=30

[Install]
WantedBy=multi-user.target
