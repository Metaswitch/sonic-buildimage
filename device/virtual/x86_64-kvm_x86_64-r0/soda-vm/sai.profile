NUM_LOGGERS=6
# Syslogs
LOGGER_1_BUFFER=SYNC
LOGGER_1_FILTER=LEVEL
LOGGER_1_FILTER_LEVELS=INFO,WARN,ERROR
LOGGER_1_FORMATTER=COLUMN
LOGGER_1_COL_SCHEMA=LEVEL:5,TEXT:0
LOGGER_1_WRITER=SYSLOG
# Normal tracing, with file filters
LOGGER_2_BUFFER=SYNC
LOGGER_2_FILTER=PATH
LOGGER_2_PATH_FILTER_IN=
LOGGER_2_PATH_FILTER_OUT=src/sai/sai_amdb.cpp,src/autogen/*,src/sai/sai_operators.cpp
LOGGER_2_FORMATTER=COLUMN
LOGGER_2_COL_SCHEMA=FILE:40,LINE:4,THREAD:8,LEVEL:5,FUNC:40,TEXT:0
LOGGER_2_WRITER=FILE
LOGGER_2_FILENAME=/var/log/syncd/soda-internal.log
# Signal logging
LOGGER_3_BUFFER=SYNC
LOGGER_3_FILTER=SIGNAL
LOGGER_3_FORMATTER=SIGNAL
LOGGER_3_WRITER=FILE
LOGGER_3_FILENAME=/var/log/syncd/soda-signal.log
LOGGER_3_DETAIL_LEVEL=DECODE
# Stat logging
LOGGER_4_BUFFER=ASYNC
LOGGER_4_FILTER=STAT
LOGGER_4_FORMATTER=COLUMN
LOGGER_4_COL_SCHEMA=TIMESTAMP:25,STAT_DESC:45,STAT_CONTEXT:35,STAT_VALUE:12
LOGGER_4_WRITER=FILE
LOGGER_4_FILENAME=/var/log/syncd/soda-stats.log
# Alarms
LOGGER_5_BUFFER=SYNC
LOGGER_5_FILTER=ALARM
LOGGER_5_FORMATTER=COLUMN
LOGGER_5_COL_SCHEMA=TEXT:0
LOGGER_5_WRITER=SYSLOG
# Stats in Redis
LOGGER_6_BUFFER=SYNC
LOGGER_6_FILTER=STAT
LOGGER_6_FORMATTER=REDIS
LOGGER_6_REDIS_TABLE=SODA_STATS_TABLE
LOGGER_6_REDIS_TABLE_KEY_SCHEMA=STAT_CONTEXT
LOGGER_6_REDIS_KEY=STAT_NAME
LOGGER_6_REDIS_VALUE=STAT_VALUE
LOGGER_6_WRITER=REDIS
LOGGER_6_REDIS_ADDRESS=/var/run/redis/redis.sock
LOGGER_6_REDIS_DATABASE=6
# Port-Lane mappings
NUM_PORTS=9
PORT_1_LANE=1
PORT_2_LANE=2
PORT_3_LANE=3
PORT_4_LANE=4
PORT_5_LANE=5
PORT_6_LANE=6
PORT_7_LANE=7
PORT_8_LANE=8
PORT_9_LANE=9
PORT_1_INTERFACE=eth1
PORT_2_INTERFACE=eth2
PORT_3_INTERFACE=eth3
PORT_4_INTERFACE=eth4
PORT_5_INTERFACE=eth5
PORT_6_INTERFACE=eth6
PORT_7_INTERFACE=eth7
PORT_8_INTERFACE=eth8
